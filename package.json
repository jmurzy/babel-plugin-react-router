{
  "name": "babel-plugin-react-router",
  "version": "0.0.1",
  "description": "Walks the AST to generate a static React Router config at compile time using Babel",
  "main": "lib/index.js",
  "author": "Jake Murzy <jake@murzy>",
  "license": "MIT",
  "homepage": "https://github.com/jmurzy/babel-plugin-react-router",
  "keywords": [
    "react-router",
    "react-router-config",
    "babel",
    "babel-plugin",
    "lazy-loading",
    "ssr",
    "prefetch",
    "relay-modern",
    "plugin",
    "universal",
    "isomorphic"
  ],
  "contributors": [
    {
      "name": "Jake Murzy",
      "email": "jake@murzy"
    }
  ],
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
